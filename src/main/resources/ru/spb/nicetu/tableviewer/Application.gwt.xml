<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='tableviewer'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.sencha.gxt.widget.core.Core"/>
  <inherits name='com.sencha.gxt.ui.GXT' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->


  <inherits name="gwtupload.GWTUpload"/>
  <inherits name="com.google.gwt.query.Query"/>
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='ru.spb.nicetu.tableviewer.client.TableViewer'/>
  <servlet path='/xlsviewer_service' class='ru.spb.nicetu.tableviewer.server.XlsViewerServiceImpl'/>
  <servlet path='/xlsUpload' class='ru.spb.nicetu.tableviewer.server.XlsUploadServlet'/>
  <!-- Specify the paths for translatable code                    -->

</module>
